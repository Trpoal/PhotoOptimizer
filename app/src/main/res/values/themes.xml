<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.PhotoOptimizer" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary_dark</item>
        <item name="colorAccent">@color/color_primary_dark</item>
    </style>

    <style name="Theme.PhotoOptimizer.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.PhotoOptimizer.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.PhotoOptimizer.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>